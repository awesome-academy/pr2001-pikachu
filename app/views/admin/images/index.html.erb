<div class="row">
  <div class="col-sm-2"></div>
  <div class="col-sm-10">
    <div class="container">
      <h3>All images of tour(<%= @images.count%>)</h3>
      <div class="image_messages">
        <%= form_for [:admin, @tour, @image], remote: true do |f| %>
          <%= render 'shared/error_messages', object: f.object %>
          <div class="field">
            Click =>
            <%= f.file_field :link %>
          </div>
          <%= f.submit "Upload", class: "btn btn-success" %>
        <% end %>

        <div class="row" id="list-image">
          <%= render @images %>
        </div>
      </div>
    </div>
  </div>
</div>
